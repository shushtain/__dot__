{
  "table1": {
    "prefix": "~table1",
    "body": ["| $1 |", "|----|", "| $2 |"]
  },
  "table2": {
    "prefix": "~table2",
    "body": ["| $1 | $2 |", "|----|----|", "| $3 | $4 |"]
  },
  "table3": {
    "prefix": "~table3",
    "body": ["| $1 | $2 | $3 |", "|----|----|----|", "| $4 | $5 | $6 |"]
  },
  "table4": {
    "prefix": "~table4",
    "body": [
      "| $1 | $2 | $3 | $4 |",
      "|----|----|----|----|",
      "| $5 | $6 | $7 | $8 |"
    ]
  },
  "details": {
    "prefix": ["~details", "~collapse", "~fold", "~summary"],
    "body": [
      "<details>",
      "<summary>${1:more}</summary>",
      "",
      "$0",
      "",
      "</details>"
    ]
  }
}
